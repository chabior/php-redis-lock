{
  "name": "chabior/php-redis-lock",
  "description": "Redis Storage for php-lock",
  "type": "library",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "chabior\\Lock\\Redis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "chabior\\Lock\\Redis\\Tests\\": "tests/src/"
    }
  },
  "require": {
    "php": "^7.2",
    "chabior/php-lock": "dev-master",
    "ext-redis": "^4.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
